{
  "name": "myProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:reset": "yarn start -- --reset-cache",
    "node:reset": "rm -rf node_modules/ && yarn cache clean && watchman watch-del-all && yarn install && cd ios && pod install && cd ..",
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode=release",
    "android:debug": "cd android && ./gradlew assembleDebug && cd ..",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:aab": "cd android && ./gradlew bundleRelease && cd ..",
    "android:dev": "react-native run-android --mode=devdebug --appIdSuffix 'dev'",
    "android:dev:release": "react-native run-android --mode=devrelease --appIdSuffix 'dev'",
    "android:dev:apk": "cd android && ./gradlew assembleDevRelease && cd ..",
    "android:dev:aab": "cd android && ./gradlew bundleDevRelease && cd ..",
    "android:stg": "react-native run-android --mode=stgdebug --appIdSuffix 'stg'",
    "android:stg:release": "react-native run-android --mode=stgrelease --appIdSuffix 'stg'",
    "android:stg:apk": "cd android && ./gradlew assembleStgRelease && cd ..",
    "android:stg:aab": "cd android && ./gradlew bundleStgRelease && cd ..",
    "android:prod": "react-native run-android --mode=proddebug",
    "android:prod:release": "react-native run-android --mode=prodrelease",
    "android:prod:apk": "cd android && ./gradlew assembleProdRelease && cd ..",
    "android:prod:aab": "cd android && ./gradlew bundleProdRelease && cd ..",
    "android:sha": "cd android && ./gradlew signingReport && cd ..",
    "android:open": "open ./android/app/build/outputs/",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "tcp": "adb reverse tcp:8081 tcp:8081",
    "pod": "cd ios && pod install && cd ..",
    "ios": "react-native run-ios",
    "ios:simulator": "react-native run-ios --simulator 'iPhone 14 Pro Max'",
    "ios:device": "react-native run-ios --device 'iPhone 14 Pro Max'",
    "ios:dev": "react-native run-ios --mode=Dev.Debug --scheme \"myProject(Dev)\"",
    "ios:stg": "react-native run-ios --mode=Stg.Debug --scheme \"myProject(Stg)\"",
    "ios:prod": "react-native run-ios --mode=Debug --scheme \"myProject\"",
    "lint": "eslint .",
    "test": "jest",
    "ios:bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest ios",
    "devices": "xcrun xctrace list devices && adb devices"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "axios": "^1.5.0",
    "expo": "^49.0.0",
    "lodash": "^4.17.21",
    "lottie-react-native": "^6.3.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-animatable": "^1.3.3",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.11.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-svg": "^13.13.0",
    "react-native-vector-icons": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/lodash": "^4.14.198",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}